{
	"id": "com.gruijter.insights2csv",
	"name": {
		"en": "Export Insights",
		"nl": "Exporteer Insights"
	},
  "version": "2.1.0",
	"sdk": 2,
	"compatibility": ">= 1.5.6",
	"author": {
		"name": "Robin de Gruijter",
		"email": "gruijter@hotmail.com"
	},
	"Copyright": "2017, 2018 Robin de Gruijter",
	"contributing": {
		"donate": {
			"paypal": {
				"username": "gruijter"
			}
		}
	},
	"description": {
		"en": "Make backups of all insights",
		"nl": "Maak backups van alle insights"
	},
	"tags": {
		"en": [ "export", "insights", "backup" ],
		"nl": [ "export", "insights", "backup" ]
	},
	"category": ["tools"],
	"images": {
		"large": "./assets/images/large.jpg",
		"small": "./assets/images/small.jpg"
	},
	"permissions": [
		"homey:manager:api"
	],
	"flow": {
		"actions": [
			{
				"id": "archive_all",
				"title": {
					"en": "Export all insights",
					"nl": "Exporteer alle insights"
				}
			},
			{
				"id": "archive_app",
				"title": {
					"en": "Export insights of:",
					"nl": "Exporteer insights van:"
				},
				"args": [
					{
						"name": "selectedApp",
						"type": "autocomplete"
					}
				]
			}
		]
	}
}
